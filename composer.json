{
    "name" : "zangra/geodis-bundle",
    "description" : "A Bundle for Geodis Labels",
    "type" : "symfony-bundle",
    "authors" : [{
        "name" : "Maxime Lambot",
        "email" : "maxime@zangra.com"
    }],
    "keywords" : [
        "Symfony",
        "api",
        "geodis",
        "labels",
